{
  "name": "yarn-up",
  "version": "1.1.0",
  "description": "Upgrade Dependencies To The Latest Versions Using Yarn.",
  "main": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "compile": "depack-dev src/bin/yarn-up -c -o compile -a",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "e": "alanode",
    "args": "argufy -o src/bin/get-args.js",
    "dev": "node src/bin"
  },
  "bin": "compile/yarn-up.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/yarn-up.git"
  },
  "keywords": [
    "yarn",
    "update",
    "upgrade",
    "dependencies",
    "latest"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/yarn-up/issues"
  },
  "homepage": "https://github.com/artdecocode/yarn-up#readme",
  "devDependencies": {
    "argufy": "^1.7.1",
    "spawncommand": "2.2.0",
    "temp-context": "2.1.3",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "documentary": "^1.32.2"
  }
}
