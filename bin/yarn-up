#!/usr/bin/env node

const { resolve } = require('path')
const up = require('../src')
const packageJson = require(resolve(process.cwd(), 'package.json'));

const { argv: [,, ...args] } = process;

(async () => {
  try {
    await up(packageJson, args)
  } catch (err) {
    console.log(err)
  }
})()
